{
    "name": "github-clone",
    "version": "1.0.0",
    "description": "A GitHub-like platform with repository management and collaboration features",
    "main": "server/index.js",
    "scripts": {
        "start": "node server/index.js",
        "dev": "nodemon server/index.js",
        "client": "cd client && npm start",
        "dev:full": "concurrently \"npm run dev\" \"npm run client\"",
        "test": "jest",
        "build": "cd client && npm run build"
    },
    "dependencies": {
        "@emotion/react": "^11.14.0",
        "@emotion/styled": "^11.14.0",
        "@mui/icons-material": "^7.1.0",
        "@mui/material": "^7.1.0",
        "@reduxjs/toolkit": "^2.8.0",
        "axios": "^1.9.0",
        "bcryptjs": "^2.4.3",
        "cors": "^2.8.5",
        "dotenv": "^16.0.3",
        "express": "^4.18.2",
        "express-rate-limit": "^6.7.0",
        "helmet": "^6.1.5",
        "jsonwebtoken": "^9.0.0",
        "mongoose": "^7.0.3",
        "react-redux": "^9.2.0",
        "react-router-dom": "^7.5.3",
        "react-syntax-highlighter": "^15.6.1",
        "remark-gfm": "^4.0.1",
        "socket.io": "^4.6.1",
        "validator": "^13.9.0"
    },
    "devDependencies": {
        "concurrently": "^8.0.1",
        "jest": "^29.5.0",
        "nodemon": "^2.0.22",
        "supertest": "^6.3.3"
    }
}
